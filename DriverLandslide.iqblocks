math.h>\n#include <string.h>\n\n\n#include \"vex.h\"\n\nusing namespace vex;\n\n// Brain should be defined by default\nbrain Brain;\n\n\n// START IQ MACROS\n#define waitUntil(condition)                                                   \\\n  do {                                                                         \\\n    wait(5, msec);                                                             \\\n  } while (!(condition))\n\n#define repeat(iterations)                                                     \\\n  for (int iterator = 0; iterator < iterations; iterator++)\n// END IQ MACROS\n\n\n// Robot configuration code.\nmotor LeftMotor = motor(PORT2, true);\nmotor RightMotor = motor(PORT8, false);\ncontroller Controller = controller();\nmotor LeftElevator = motor(PORT6, false);\nmotor RightElevator = motor(PORT7, false);\nmotor LeftRollers = motor(PORT9, false);\nmotor RightRollers = motor(PORT12, false);\ntouchled Borgor = touchled(PORT11);\ngyro Gyro4 = gyro(PORT4);\ncolorsensor Grid = colorsensor(PORT5);\n\n\n\n// Generated code.\n\n\n\n// define variable for remote controller enable/disable\nbool RemoteControlCodeEnabled = true;\n\n\n\n// Include the IQ Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\n// User defined function\nvoid myblockfunction_spit();\n// User defined function\nvoid myblockfunction_succ();\n// User defined function\nvoid myblockfunction_hold();\n// User defined function\nvoid myblockfunction_up();\n// User defined function\nvoid myblockfunction_down();\n// User defined function\nvoid myblockfunction_goto_x_y(double myblockfunction_goto_x_y__x, double myblockfunction_goto_x_y__y);\n\nfloat myVariable, deadBand, driveTorque, JawsOpen, ArmDown, ArmDownPosWhenJawsOp, ArmDownPosWhenJawsCl, ArmTopAngle, ElevatorTop, ElevatorMid, x, y, theta, drift, s, theta2;\n\nbool rollers, autopilot;\n\n// User defined function\nvoid myblockfunction_spit() {\n  LeftRollers.setVelocity(100.0, percent);\n  Ri